<page layout="3columns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="default_head_blocks"/>
    <body>
        <referenceBlock name="header.links">
            <block class="Magento\Framework\View\Element\Html\Link" name="button.link"  before="-">
                <arguments>
                    <argument name="label" xsi:type="string">create a delivery service Account</argument>
                    <argument name="path" xsi:type="string">inscription/account/create</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceBlock name="header.links">
            <block class="Magento\Framework\View\Element\Html\Link" name="button2.link"  before="-">
                <arguments>
                    <argument name="label" xsi:type="string">Sign In as a delivery service Client</argument>
                    <argument name="path" xsi:type="string">admin</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
