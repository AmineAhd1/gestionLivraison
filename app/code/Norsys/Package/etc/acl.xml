<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Norsys_Package::Package" title="Package" sortOrder="10">
                    <resource id="Norsys_Package::Packages" title="View Packages" sortOrder="50"/>
                    <resource id="Norsys_Package::packages_management" title="Packages Management" sortOrder="51"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Norsys_Package::package_config" title="package Config Section"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
